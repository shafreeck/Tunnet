{
  "$schema": "../node_modules/@tauri-apps/cli/config.schema.json",
  "productName": "Tunnet",
  "version": "0.1.0",
  "identifier": "run.tunnet.app",
  "build": {
    "frontendDist": "../out",
    "devUrl": "http://127.0.0.1:4000",
    "beforeDevCommand": "npm run dev",
    "beforeBuildCommand": "npm run build"
  },
  "app": {
    "windows": [
      {
        "label": "main",
        "title": "Tunnet",
        "width": 1000,
        "height": 720,
        "resizable": true,
        "fullscreen": false,
        "transparent": true,
        "decorations": false,
        "titleBarStyle": "Overlay",
        "hiddenTitle": true,
        "shadow": false
      },
      {
        "label": "tray",
        "url": "/tray",
        "width": 320,
        "height": 450,
        "resizable": false,
        "transparent": true,
        "decorations": false,
        "alwaysOnTop": true,
        "visible": false,
        "skipTaskbar": true,
        "shadow": false
      }
    ],
    "security": {
      "csp": null
    },
    "macOSPrivateApi": true
  },
  "plugins": {
    "deep-link": {
      "desktop": {
        "schemes": [
          "tunnet",
          "sing-box"
        ]
      }
    },
    "updater": {
      "pubkey": "dW50cnVzdGVkIGNvbW1lbnQ6IG1pbmlzaWduIHB1YmxpYyBrZXk6IEQxNkQyODk1QzE4OUQ0QjkKUldTNTFJbkJsU2h0MFk2NUxMY2VBUTY1c252NW05cHZ5YzArZmlWTFVkMkZGWXRDemNZWTE1ZnMK",
      "endpoints": [
        "https://github.com/shafreeck/Tunnet/releases/latest/download/latest.json"
      ]
    }
  },
  "bundle": {
    "active": true,
    "createUpdaterArtifacts": true,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico",
      "icons/icon.png"
    ],
    "resources": [
      "resources/geoip-cn.srs",
      "resources/geosite-cn.srs",
      "resources/tunnet-helper"
    ],
    "macOS": {
      "infoPlist": "Info.plist"
    },
    "iOS": {
      "developmentTeam": "JSG9S9M456"
    }
  }
}