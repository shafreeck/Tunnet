{
  "$schema": "../node_modules/@tauri-apps/cli/config.schema.json",
  "productName": "Tunnet",
  "version": "0.1.0",
  "identifier": "com.shafreeck.tunnet",
  "build": {
    "frontendDist": "../out",
    "devUrl": "http://localhost:4000",
    "beforeDevCommand": "npm run dev",
    "beforeBuildCommand": "npm run build"
  },
  "app": {
    "windows": [
      {
        "title": "Tunnet",
        "width": 1000,
        "height": 700,
        "resizable": true,
        "fullscreen": false,
        "transparent": true,
        "decorations": false,
        "titleBarStyle": "Overlay",
        "hiddenTitle": true,
        "shadow": false
      },
      {
        "label": "tray",
        "url": "/tray",
        "width": 320,
        "height": 480,
        "resizable": false,
        "transparent": true,
        "decorations": false,
        "alwaysOnTop": true,
        "visible": false,
        "skipTaskbar": true,
        "shadow": false
      }
    ],
    "security": {
      "csp": null
    },
    "macOSPrivateApi": true
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico",
      "icons/icon.png"
    ],
    "resources": [
      "resources/geoip-cn.srs",
      "resources/geosite-cn.srs",
      "resources/sing-box",
      "resources/tunnet-helper"
    ],
    "iOS": {
      "developmentTeam": "JSG9S9M456"
    }
  }
}